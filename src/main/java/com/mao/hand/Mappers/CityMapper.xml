<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.mao.hand.Beans.City"> 
	<resultMap type="City" id="CityResult">	
	    <id	property="city_id" column="city_id"/>	      
		<result property="city" column="city"/> 
		<result property="country_id" column="country_id"/>
		<result property="last_update" column="last_update"/>

	</resultMap>
  	
  	<select id ="findAllCity" resultMap ="CityResult">
  		select * from city
  	</select>
</mapper>