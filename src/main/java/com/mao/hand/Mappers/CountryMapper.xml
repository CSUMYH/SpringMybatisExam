<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.mao.hand.Beans.Country"> 
	<resultMap type="Country" id="CountryResult">	
	    <id	property="country_id" column="country_id"/>	      
		<result property="country" column="country"/>
		<result property="last_update" column="last_update"/>

	</resultMap>
  	
  	<select id ="findAllCountry" resultMap ="CountryResult">
  		select * from country
  	</select>
</mapper>