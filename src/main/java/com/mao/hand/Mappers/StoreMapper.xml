<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.mao.hand.Beans.Store"> 
	<resultMap type="Store" id="StoreResult">	
	    <id	property="store_id" column="store_id"/>	      
		<result property="manager_staff_id" column="manager_staff_id"/>
		<result property="address_id" column="address_id"/>
		<result property="last_update" column="last_update"/>

	</resultMap>
  	
  	<select id ="findAllStore" resultMap ="StoreResult">
  		select * from store
  	</select>
</mapper>